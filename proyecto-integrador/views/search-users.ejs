<%- include('./partials/head') %>
	<%- include('./partials/header') %>
		<div class="container products-wrapper">
			<div class="row">
				<% if (productos.length> 0) { %>
					<div class="col-12">
						<h2 class="products-title">Estos usuarios coinciden con tu búsqueda</h2>
					</div>
					<!-- Show the "ultimos" products -->
					<% for( let i=0; i < usuarios.length; i++ ) { %>
						<div class="col-12 col-sm-6 col-lg-3">
							<section class="product-box">
								<a href="/camisetas/id/<%= usuarios[i].id %>">
									<figure class="product-box_image">
										<img src="<%= usuarios[i].foto_perfil %>" alt="">
									</figure>
									<article class="product-box_data">
										<h2>
											<%= usuarios[i].usuario %>
										</h2>
										
								
									</article>
								</a>
							</section>
						</div>
						<% } %>
							<% } else {%>
								<div class="col-12">
									<h2 class="products-title">No hay coincidencias de búsqueda</h2>
								</div>
								<% } %>

			</div>
		</div>

		<%- include('./partials/footer') %>